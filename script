// ==UserScript==

// @name         siteExecutor

// @namespace    http://tampermonkey.net/

// @version      4.0

// @description  doesn't open a very cool website

// @author       purefishmonke#3932 (discord)

// @match        https://*/*

// ==/UserScript==



function getTargetUrl() {



	const targetUrl = "https://sites.google.com/view/verycool-site"



	return targetUrl;

}



(function() {

	'use strict';



	document.onkeydown = evt => {

		evt = evt || window.event;

		if(evt.shiftKey && evt.altKey && evt.keyCode == 71){

			window.open(getTargetUrl(), '_blank');

		}

	};

})();



'use strict';

document.body.insertAdjacentHTML("afterbegin",'<div id=button style="background-color: rgb(255, 64, 0);position: fixed;display: flex;opacity: 0.66;margin: 1em;z-index: 2147483647"<body>ㅤ➕</body><svg width="12" height="8">"/></svg></div>');

var btn=document.getElementById("button");



var action=function(){

    alert('Hello! To open the webite press SHIFT+ALT+G thank you!\nthis button will be enabled on every website until I make a fix sorry about that, for now just disable the script\n there is an update log on the website, if this script needs an update it will be announced what version it is moving to (Current version is v4.0)');

}

btn.onclick=action;
